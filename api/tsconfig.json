{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": [
      "@cloudflare/workers-types"
    ],
    "outDir": "dist"
  },
  "include": [
    "src/**/*.ts"
  ]
}
